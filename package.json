{
  "name": "openai-chatkit-starter-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^22.14.0",
    "npm": "^10.9.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "heroku-prebuild": "NODE_ENV=development npm ci",
    "heroku-postbuild": "npm run build && NODE_ENV=production npm ci",
    "heroku-cleanup": "find . -depth -maxdepth 1 -not -name '.' -not -name '*.d.ts' -not -name '.next' -not -name 'public' -not -name 'node_modules' -print | xargs rm -rv"
  },
  "dependencies": {
    "@openai/chatkit-react": ">=1.1.1 <2.0.0",
    "next": "^15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "1.30.1",
    "@tailwindcss/oxide-linux-x64-gnu": "4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
